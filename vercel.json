{
  "rewrites": [
    { "source": "/container/(.*)", "destination": "/container/$1" },
    { "source": "/marketing/(.*)", "destination": "/marketing/$1" },
    { "source": "/auth/(.*)", "destination": "/auth/$1" },
    { "source": "/dashboard/(.*)", "destination": "/dashboard/$1" },
    { "source": "/usermanagement/(.*)", "destination": "/usermanagement/$1" },
    { "source": "/billingdashboard/(.*)", "destination": "/billingdashboard/$1" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist"
} 